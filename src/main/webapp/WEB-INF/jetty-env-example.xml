<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE Configure PUBLIC "-//Mort Bay Consulting//DTD Configure//EN" "http://www.eclipse.org/jetty/configure_9_0.dtd">
<Configure>
    <New id="DSTest" class="org.eclipse.jetty.plus.jndi.Resource">
        <Arg>jdbc/DSTest</Arg>
        <Arg>
            <New class="org.postgresql.ds.PGSimpleDataSource">
                <Set name="Url">jdbc:postgresql://values.amazonaws.com:5432/values?sslmode=require</Set>
                <Set name="User">values</Set>
                <Set name="Password">values</Set>
                <Set name="DatabaseName">values?sslmode=require</Set>
                <Set name="ServerName">values.amazonaws.com</Set>
                <Set name="PortNumber">5432</Set>
            </New>
        </Arg>
    </New>
</Configure>

